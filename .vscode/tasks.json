{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build my project",
            "type": "shell",
            "command": "clang++",
            "args": [
                "-Wall",
                "-Wextra",
                "-g3",
                "-std=c++17",
                "-I/usr/local/Cellar/cgal/5.6/include",
                "-I/usr/local/include",
                "-I/Users/ciaratorguson/Desktop/matlab/extern/include",
                "-I/Applications/MATLAB_R2023b.app/extern/include",
                "${workspaceFolder}/main.cpp",
                "-o",
                "${workspaceFolder}/output/main",
                "-L/usr/local/lib",
                "-lCGAL",
                "-lboost_thread",
                "-lgmp"
            ],
            "group": "build"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}